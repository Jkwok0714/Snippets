{
  "manifest_version": 2,
  "name": "Tab Targeter",
  "description": "Targeting the tabbicons",
  "version": "0.1",
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": ["http://localhost/*", "<all_urls>", "tabs", "tabCapture", "activeTab"]
}
